    [[JMRequestManager sharedManager] POST:kUrlLiveExitRoom parameters:params completion:^(JMBaseResponse *response) {
        if(response.error){
            [JMProgressHelper toastInWindowWithMessage:response.errorMsg];
        }else{
            
        }
    }];
}

